<p id="notice"><%= notice %></p>

<h1>Video Likes</h1>

<table>
  <thead>
    <tr>
      <th>Video</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_likes.each do |video_like| %>
      <tr>
        <td><%= video_like.video %></td>
        <td><%= video_like.user %></td>
        <td><%= link_to 'Show', video_like %></td>
        <td><%= link_to 'Edit', edit_video_like_path(video_like) %></td>
        <td><%= link_to 'Destroy', video_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video Like', new_video_like_path %>
